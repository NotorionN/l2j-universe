    <!--
		Изменен: 4:02/16.04.2013, Mazaffaka
		Название: Камень Обработки Головного Убора
		Описание: Витаминный предмет
		Может изменить облик головного убора. Облик можно копировать с предметов, занимающих аналогичное количество ячеек. Требуется предмет для извлечения облика, который при этом уничтожается. Параметры обработанного предмета не меняются. Использовать двойным щелчком мыши. На головные уборы, занимающие 1 ячейку, расходуется 125 000 аден, 2 ячейки - 200 000 аден. Нельзя обменять/выбросить. Можно уничтожить. Можно положить в личное хранилище.

		Внимание: если изменить внешний вид женского головного убора, то он станет доступен персонажу мужского пола; если к обработанному головному убору применить кристалл души или свиток модификации, то его внешний вид вернется в исходное состояние.
		При добавлении кулона внешний вид предмета также вернется в исходное состояние.
	-->
    <etcitem id="22708" name="Камень Обработки Головного Убора">
        <set name="icon" value="BranchSys3.Icon.g_shape_shifting_hair2_stone"/>
        <set name="type" value="OTHER"/>
        <set name="destroyable" value="false"/>
        <set name="dropable" value="false"/>
        <set name="augmentable" value="false"/>
        <set name="enchantable" value="false"/>
        <set name="attributable" value="false"/>
        <set name="sellable" value="false"/>
        <set name="tradeable" value="false"/>
        <set name="stackable" value="true"/>
        <set name="weight" value="0"/>
        <cond msgId="1508">
            <not>
                <player olympiad="true"/>
            </not>
        </cond>
    </etcitem>
