<list>
    <etcitem id="35292" name="Свиток Телепорта: Крепость Гильотина">
        <!-- Магический свиток, телепортирующий в Крепость Гильотина,. Нельзя продать -->
        <set name="class" value="CONSUMABLE"/>
        <set name="crystal_type" value="NONE"/>
        <set name="dropable" value="false"/>
        <set name="icon" value="icon.etc_scroll_of_return_i00"/>
        <set name="price" value="35000"/>
        <set name="stackable" value="true"/>
